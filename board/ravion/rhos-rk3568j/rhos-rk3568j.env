ipaddr=192.168.5.101
netmask=255.255.255.0
serverip=192.168.5.254
gatewayip=192.168.5.254
board=kitsbimx8
fdt_file=rk3568-rhos-kitsbimx8.dtb

ravboot=tftpboot ${loadaddr} ${serverip}:/boot/bscript.img; source ${loadaddr}
nvme_sysboot=sysboot nvme 0:2 any ${scriptaddr} /syslinux.conf
usb_sysboot=sysboot usb 0:1 any ${scriptaddr} /syslinux.conf
boot0_install=echo "Install u-boot-rockchip.bin from SD-card to BOOT0 eMMC partition"

